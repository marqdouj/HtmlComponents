@rendermode InteractiveAuto

<div class="navmenu">
    <input type="checkbox" title="Menu expand/collapse toggle" id="navmenu-toggle" class="navmenu-icon" />
    <label for="navmenu-toggle" class="navmenu-icon"><FluentIcon Value="@(new Icons.Regular.Size20.Navigation())" Color="Color.Fill" /></label>
    <nav class="sitenav" aria-labelledby="main-menu">
        <FluentNavMenu Id="main-menu" Width="250" Collapsible="true" Title="Navigation menu" @bind-Expanded="expanded" CustomToggle="true">
            <FluentNavLink Href="/" Match="NavLinkMatch.All" Icon="@(new Icons.Regular.Size20.Home())" IconColor="Color.Accent">Home</FluentNavLink>
            <FluentNavLink Href="avatar" Icon="@(new Icons.Regular.Size20.ImageCircle())" IconColor="Color.Accent">Avatar</FluentNavLink>
            <FluentNavLink Href="resize" Icon="@(new Icons.Regular.Size20.Resize())" IconColor="Color.Accent">Resize Observer</FluentNavLink>
            <FluentNavLink Href="slide-show-demo" Icon="@(new Icons.Regular.Size20.SlideArrowRight())" IconColor="Color.Accent">Slide Show</FluentNavLink>
            <FluentNavLink Href="ui-model" Icon="@(new Icons.Regular.Size20.Screenshot())" IconColor="Color.Accent">UI Model</FluentNavLink>
        </FluentNavMenu>
    </nav>
</div>

@code {
    private bool expanded = true;

}
